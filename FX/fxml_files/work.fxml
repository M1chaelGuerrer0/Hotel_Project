<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gooncentral.workController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Button fx:id="deletebutton" mnemonicParsing="false" onAction="#Delete" text="Delete" translateX="-50.0" translateY="48.0" />
        <Button fx:id="Refresh" mnemonicParsing="false" onAction="#Refresh" text="Refresh" translateX="50.0" translateY="1.0" />
        <TableView fx:id="table" prefHeight="200.0" prefWidth="700.0">
            <columns>
                <TableColumn fx:id="firstName" prefWidth="100.0" text="First name" />
                <TableColumn fx:id="lastName" prefWidth="100.0" text="Last name" />
                <TableColumn fx:id="Id" prefWidth="50.0" text="Id" />
                <TableColumn fx:id="email" prefWidth="200.0" text="Email Address" />
                <TableColumn fx:id="phone" prefWidth="150.0" text="Phone number" />
                <TableColumn fx:id="address" prefWidth="150.0" text="Address" />
                <TableColumn fx:id="zip" prefWidth="100.0" text="Zip code" />
            </columns>
        </TableView>

        <TableView fx:id="table0" prefHeight="400.0" prefWidth="700.0">
            <columns>
                <TableColumn fx:id="firstName0" prefWidth="100.0" text="First name" />
                <TableColumn fx:id="lastName0" prefWidth="100.0" text="Last name" />
                <TableColumn fx:id="Id0" prefWidth="50.0" text="Id" />
                <TableColumn fx:id="email0" prefWidth="200.0" text="Email Address" />
                <TableColumn fx:id="phone0" prefWidth="150.0" text="Phone number" />
                <TableColumn fx:id="address0" prefWidth="150.0" text="Address" />
                <TableColumn fx:id="zip0" prefWidth="100.0" text="Zip code" />
            </columns>
        </TableView>


    </children>
</VBox>
